    "name": "Mantas Miliukas",
    "email": "mantasm@wix.com"
  },
  "main": "mocha.js",
  "files": [
    "*.js"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "start": "npm run test:mocha -- --watch",
    "test": "export EYES_BATCH_UUID=$(uuidgen) && npm run test:mocha && npm run test:jest && npm run test:jasmine",
    "test:mocha": "mocha ./test/mocha.spec.js --reporter mocha-env-reporter",
    "test:jest": "jest ./test/jest.spec.js",
    "test:jasmine": "jasmine ./test/jasmine.spec.js",
    "build": "eslint *.js test",
    "release": ":"
  },
  "devDependencies": {
    "chai": "~4.1.0",
    "eslint": "^4.19.1",
    "eslint-config-wix": "^1.1.18",
    "jasmine": "^3.1.0",
    "jest": "^22.4.3",
    "mocha": "^5.1.1",
    "mocha-env-reporter": "^3.0.0"
  },
  "dependencies": {
    "eyes.images": "0.0.50",
    "uuid": "^3.2.1"
  },
  "eslintConfig": {
    "extends": "wix/esnext"
  }
}
